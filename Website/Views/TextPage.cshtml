@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.TextPage>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "websiteMaster.cshtml";
}


@section heading {
    <h2 class="subheading">@(Model.Name)</h2>
}

<!-- Post Content -->
<article>
    <div class="container">
        <div class="row">
            <div class="col-lg-10 col-md-10 mx-auto">
                @(Model.Value("text"))
            </div>
        </div>
    </div>
</article>